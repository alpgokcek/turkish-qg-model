echo -e "\nPROMPT='%({%F{yellow}%}.)$USER @ %{$fg[white]%}%M %{$fg_bold[red]%}âžœ %{$fg_bold[green]%}%p %{$fg[cyan]%}%c %{$fg_bold[blue]%}$(git_prompt_info)%{$fg_bold[blue]%} % %{$reset_color%}'" >> .zshrc
source ~/.zshrc
